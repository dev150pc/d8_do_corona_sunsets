      <div id="header_links" class="header_links_m">
        <ul>
          <li class="submenu_mobile_m"><img class="submenu_icon_m" src="/{{ directory }}/images/person-circle-outline-white.svg"
              alt="Person Circle Outline">
            {{ drupal_block('block_name_user') }}<img id="second_submenu_m" class="arrow_right_m" src="/{{ directory }}/images/arrow right.svg"
              alt="Flecha derecha">
          </li>
          {% for item in header_link_menu %}
            <li><a class="link_menu_corona" href="{{ item['url'] }}">{{ item['title'] }}</a></li>
        {% endfor %}
        </ul>
      </div>
      <div id="header_registro" class="header_registro_m">
        <div class="header_registrate_m header_registrate_logged_m"><a href="/subir-codigos">VER MIS FACTURAS</a></div>
