          <div class="header_div2_m header_div2_logged_m">
            {{ drupal_block('block_name_user') }}
          </div>
          <img src="/{{ directory }}/images/arrow-down.svg" class="header_img2_m" alt="Flecha Abajo">
          <div id="submenu_m" class="submenu_m">
            <ul>
              <li><img id="third_submenu_m" class="arrow_left_m" src="/{{ directory }}/images/arrow-left.svg"
                  alt="Flecha Izquierda"><img class="secondary_submenu_icon_m"
                  src="/{{ directory }}/images/person-circle-outline-white.svg" alt="Person Circle Outline">
                {{ drupal_block('block_name_user') }}
